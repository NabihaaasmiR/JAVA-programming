import java.util.*;
import java.io.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),res=1;
        int m=sc.nextInt();
        for(int i=0;i<m;i++)
        {
            res=res*n;
        }
        System.out.println(res);

    }
}
