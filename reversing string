import java.util.*;
class Main {
    void first(String n)
{
    for(int i=n.length()-1;i>=0;i--)
    {
        char ch=n.charAt(i);
        System.out.println(ch);
        
}
}
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
       Main obj=new Main();
       obj.first(a);
    }
}
