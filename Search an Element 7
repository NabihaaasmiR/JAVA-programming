import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      boolean flag=false;
      int[] arr=new int[n];
      for(int i=0;i<n;i++)
      {
        arr[i]=sc.nextInt();
      }
      int ele=sc.nextInt();
      for(int i=0;i<n;i++)
      {
          if(arr[i]==ele)
          {
            flag=true;
          }
        }
      
      if(flag)
        System.out.println(ele+" is presented in an array.");
      else
        System.out.println(ele+" is not presented in an array.");
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
