import java.util.*;
class Main {
    int add(int n,int m)
{
        return (n+m);
}
    double add(double n,double m)
    {   
        return (n+m);
    }
    void display()
    {
         Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        double c=sc.nextDouble();
        double d=sc.nextDouble();
        System.out.println(add(a,b));
        System.out.println(add(c,d));
    }

    public static void main(String[] args) {
       
       Main obj=new Main();
       obj.display();
    }
}
