import java.util.*;
class Main {
    void display()
    {
         Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println(a+" "+b);
    }

    public static void main(String[] args) {
       
       Main obj=new Main();
       obj.display();
    }
}
